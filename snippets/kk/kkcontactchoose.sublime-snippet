// 选择联系人 
kk.contact.choose({
  mode: ${1:1} // 选择类型模式 默认值是1(人), 可选值还有2(电话号码),3(邮件)
}, function (res) {
  // res 联系人信息数组，可查看
  // http://kk5.landray.com.cn:6789/jssdk/index.html#contact
  ${2}
}, function (code, msg) {
  console.log('选择联系人失败，错误代码是' + code + '，错误信息是' + msg);
});
]]></content>
