// 加密字符串
kk.crypto.encrypt({
  text: '${1:需要加密的字符串}', 
  key: '${2:mykey}'
}, function (res) {
  /* res 示例
    {
      retCode:'0',
      text:'54vsRgkdgRk0bvBcPZYdx3jwYTevd8591lXSBBctp7k=' // 加密后的字符串
    }
  */
  ${3}
}, function (code, msg) {
  console.log('加密字符串失败，错误代码是' + code + '，错误信息是' + msg);
});
]]></content>
